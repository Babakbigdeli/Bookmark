{
  "name": "BookmarkApp",
  "version": "1.0.0",
  "description": "An online application to help you keep track of your reading",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "node build/build.js && mv _redirects dist",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Babakbigdeli/BookmarkApp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Babakbigdeli/BookmarkApp/issues"
  },
  "homepage": "https://github.com/Babakbigdeli/BookmarkApp#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.6.0",
    "concurrently": "^6.0.1",
    "express": "^4.17.1",
    "heroku": "^7.53.0",
    "if-env": "^1.0.4",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.5",
    "react-bootstrap": "^1.5.2",
    "react-icons": "^4.2.0",
    "react-json-pretty": "^2.2.0"
  }
}
